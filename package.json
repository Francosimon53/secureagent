    "playwright-core": "^1.58.1",
    "puppeteer-core": "^24.36.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.0.0",
    "@vercel/node": "^5.5.28",
